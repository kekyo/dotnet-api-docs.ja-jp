      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.WindowsStreamSecurityElement">
          <source>Represents a configuration element that is used to specify settings for Windows stream security for a binding.</source>
          <target state="translated">バインディングの Windows ストリーム セキュリティ設定の指定に使用される構成要素を表します。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.WindowsStreamSecurityElement">
          <source>This class cannot be inherited.</source>
          <target state="translated">このクラスは継承できません。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsStreamSecurityElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WindowsStreamSecurityElement" /&gt;</ph> class.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.WindowsStreamSecurityElement" /&gt;</ph> クラスの新しいインスタンスを初期化します。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ApplyConfiguration(System.ServiceModel.Channels.BindingElement)">
          <source>A binding element.</source>
          <target state="translated">バインディング要素。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ApplyConfiguration(System.ServiceModel.Channels.BindingElement)">
          <source>Applies the content of a specified binding element to this binding configuration element.</source>
          <target state="translated">指定されたバインディング要素の内容を、このバインディング構成セクションに適用します。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.BindingElementType">
          <source>Gets the <ph id="ph1">&lt;see cref="T:System.Type" /&gt;</ph> object that represents the binding element.</source>
          <target state="translated">バインディング要素を表す <ph id="ph1">&lt;see cref="T:System.Type" /&gt;</ph> オブジェクトを取得します。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.BindingElementType">
          <source>A <ph id="ph1">&lt;see cref="T:System.Type" /&gt;</ph> object that represents the binding type.</source>
          <target state="translated">バインディングの種類を表す <ph id="ph1">&lt;see cref="T:System.Type" /&gt;</ph> オブジェクト。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsStreamSecurityElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>The configuration element to be copied.</source>
          <target state="translated">コピーされる構成要素。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.WindowsStreamSecurityElement.CopyFrom(System.ServiceModel.Configuration.ServiceModelExtensionElement)">
          <source>Copies the content of a specified configuration element to this configuration element.</source>
          <target state="translated">指定された構成要素の内容をこの構成要素にコピーします。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>Gets or sets the message-level security used.</source>
          <target state="translated">使用されるメッセージレベルのセキュリティを取得または設定します。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>A <ph id="ph1">&lt;see cref="T:System.Net.Security.ProtectionLevel" /&gt;</ph> that specifies the message-level security used.</source>
          <target state="translated">使用されるメッセージレベルのセキュリティを指定する <ph id="ph1">&lt;see cref="T:System.Net.Security.ProtectionLevel" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>The default value is <ph id="ph1">&lt;see cref="F:System.Net.Security.ProtectionLevel.EncryptAndSign" /&gt;</ph> .</source>
          <target state="translated">既定値は <ph id="ph1">&lt;see cref="F:System.Net.Security.ProtectionLevel.EncryptAndSign" /&gt;</ph> です。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>This property specifies the message-level security used for signing and encryption.</source>
          <target state="translated">このプロパティは、署名および暗号化で使用されるメッセージレベルのセキュリティを指定します。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>Signing messages mitigates the risk of a third party tampering with the message while it is being transferred.</source>
          <target state="translated">メッセージに署名すると、転送中のメッセージが第三者によって改ざんされるリスクを軽減します。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.WindowsStreamSecurityElement.ProtectionLevel">
          <source>Encryption provides data-level privacy during transport.</source>
          <target state="translated">暗号化により、転送中にデータレベルのプライバシーが提供されます。</target>       </trans-unit>
