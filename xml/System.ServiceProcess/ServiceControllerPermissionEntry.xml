        <trans-unit id="118" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceProcess.ServiceControllerPermissionEntry.ServiceName">
          <source>The name of the service.</source>
          <target state="translated">サービスの名前。</target>       </trans-unit>
