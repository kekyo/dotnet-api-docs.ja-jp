        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.Text.TextUnit.Character">
          <source>Specifies that the text unit is one character in length.</source>
          <target state="translated">テキスト単位の長さが 1 文字であることを指定します。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Automation.Text.TextUnit.Document">
          <source>Specifies that the text unit is an entire document in length.</source>
